    stage('Checkout') {
      steps {
        echo 'Checkout'
      }
    }

    stage('Build') {
      steps {
        echo 'Build'
      }
    }

    stage('Run Tests') {
      steps {
        echo 'Run Tests'
      }
    }

    stage('SonarQube Analysis') {
      steps {
        echo 'SonarQube Analysis'
      }
    }

  }
}